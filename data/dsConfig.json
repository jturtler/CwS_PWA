		"btn_SearchByVoucher": { 

			"defaultLabel": "Search",
			"buttonType": "textButton",
			"onClick": [
				{
					"actionType": "sendToWS",
					"url": "/api/dc/searchByVoucher",
					"payloadHeader": [ "usr", "pwd" ],
					"payloadBody": [ "voucherCode" ],
					"wsNACase": {
						"note": "display message"
					}
				},
				{
					"actionType": "processWSResult",
					"resultCase": {
						"foundOne": [
							"closeCurrentBlock",
							{
								"actionType": "openBlock",
								"blockId": "blockRedeemFormInputs",
								"hideCase": "foundOneWithVoucher",
								"actionParameters": {
									"voucherData": "$${resultJson}",
									"otherPassInData": { "voucher": "43" }
								}
							}
						],
						"notFound": [
						],
						"foundMultiple": []
					}
				}
			]
		},

		"btn_SubmitRedeem": {

			"defaultLabel": "Redeem",
			"buttonType": "textButton",
			"onClick": [
				{
					"actionType": "sendToWS",
					"url": "/api/dc/redeem",
					"alertResult": "true",
					"redeemListInsert": "true", 
					"payloadHeader": [ "usr", "pwd" ],
					"payloadBody": [ "voucherCode" ],
					"wsNACase": {
						"note": "display message"
					}
				},
				{
					"actionType": "processWSResult",
					"resultCase": {
						"success": [
							"closeCurrentBlock",							
							{
								"actionType": "openBlock",
								"blockId": "blockRedeemList",
								"passData": {
									"voucherData": "$${resultJson}"
								}
							}
						],
						"fail": [
							{
								"actionType": "alertMsg",
								"message": "Fail"
							}
						],
						"offline": [
							"closeCurrentBlock",							
							{
								"actionType": "openBlock",
								"blockId": "blockRedeemList"
							}							
						]
					}
				}
			]			
		},

		"btn_OpenRedeemFormBySearchList": { 

			"defaultLabel": "Open Redeem Form",
			"buttonType": "textButton",
