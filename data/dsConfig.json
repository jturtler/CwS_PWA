						"note": "display message"
					}
				},
				{
					"actionType": "processWSResult",
					"resultCase": {
						"foundOne": [
							"closeCurrentBlock",
							{
								"actionType": "openBlock",
								"blockId": "blockRedeemFormInputs",
								"showCase": "foundOneWithVoucher",
								"hideCase": "foundOneWithVoucher"
							}
						],
						"notFound": [
							{
								"actionType": "openBlock",
								"blockId": "blockRedeemFormInputs",
								"showCase": "WalkInUserShow",
								"hideCase": "WalkInUserShow"
							},
							{
								"actionType": "filledData",
								"fromBlockId": "blockSearchByDetails",
								"toBlockId": "blockRedeemFormInputs",
								"dataItems": ["walkIn_firstName", "walkIn_lastName","walkIn_birthDistrict", "walkIn_birthOrder", "walkIn_age"]
							},
							{
								"actionType": "closeBlock",
								"blockId": "blockSearchByDetails"
							}
						],
						"foundOneWithoutVoucher": [
							"closeCurrentBlock",
							{
								"actionType": "openBlock",
								"blockId": "blockRedeemFormInputs",
								"showCase": "WalkInUserFoundOneWithoutVoucher",
								"hideCase": "WalkInUserFoundOneWithoutVoucher"
							}
						]
					}
				}
			]			
		}
	},

